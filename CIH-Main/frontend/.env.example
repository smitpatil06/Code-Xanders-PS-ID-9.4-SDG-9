# Environment Variables
# Create a .env file in the frontend directory and add these variables

# Backend API URL
VITE_API_URL=http://localhost:8000

# WebSocket URL (if using real-time data)
VITE_WS_URL=ws://localhost:8000/ws

# API Polling Interval (milliseconds)
VITE_POLLING_INTERVAL=5000

# Enable/Disable Mock Data (for development without backend)
VITE_USE_MOCK_DATA=false
